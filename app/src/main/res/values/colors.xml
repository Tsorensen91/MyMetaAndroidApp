<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorPrimary">#b81570</color>
    <color name="colorPrimaryDark">#01b0ad</color>
    <color name="colorAccent">#ffffff</color>
    <color name="colorMagenta">#B81570</color>
    <color name="colorTeal">#01B0AD</color>
    <color name="colorWhite">#FFFFFF</color>
    <color name="colorGrey">#C0C0C0</color>
    <color name="colorLightGrey">#EBEBEB</color>

    <style name="SCBSwitch" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/colorMagenta</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">#f1f1f1
        </item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">@color/colorMagenta</item>
    </style>

</resources>
